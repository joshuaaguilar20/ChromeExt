 {
    "name": "Annoying Window Remover",
    "version": "1.0",
    "description": "Allows you to delete entire dom elements such as ads blocking video displays. ",
    "permissions":["storage"],
    "manifest_version": 2
 	"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
      "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }
